{
  "name": "eventflow-backend",
  "version": "1.0.0",
  "description": "GraphQL API for EventFlow - An Event Management Platform",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "docker compose -f docker-compose.dev.yml up --build",
    "dev:full": "docker compose -f docker-compose.dev.yml up --build -d mongodb && docker compose -f docker-compose.dev.yml up --build backend",
    "build": "npm run clean && tsc && node export-schema.js",
    "export-schema": "node export-schema.js",
    "clean": "rimraf dist",
    "db:start": "docker compose up -d mongodb",
    "db:stop": "docker compose stop mongodb",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:check": "eslint . --format json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "seed": "node dist/seed.js",
    "test:ci": "jest --ci --coverage",
    "check": "npm run typecheck && npm run lint && npm run format:check",
    "check:duplication": "jscpd src --ignore \"**/__tests__/**,**/*.test.ts,**/*.spec.ts\"",
    "check:deps": "depcheck",
    "check:updates": "ncu",
    "check:unused": "ts-prune --ignore \"src/__tests__/.*\"",
    "validate": "npm run typecheck && npm run lint && npm run format:check && npm run check:duplication && npm run check:deps && npm run check:unused"
  },
  "keywords": [
    "graphql",
    "apollo",
    "express",
    "event-management"
  ],
  "author": "",
  "license": "ISC",
  "depcheck": {
    "ignores": [
      "@types/jest",
      "@types/supertest",
      "rimraf",
      "supertest",
      "ts-jest",
      "ts-node",
      "ts-node-dev"
    ]
  },
  "dependencies": {
    "@apollo/utils.keyvaluecache": "^4.0.0",
    "@as-integrations/express5": "^1.1.2",
    "@graphql-tools/schema": "^10.0.31",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.6",
    "dataloader": "^2.2.3",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "graphql": "^16.12.0",
    "graphql-query-complexity": "^1.1.0",
    "graphql-subscriptions": "^3.0.0",
    "graphql-tag": "^2.12.6",
    "graphql-ws": "^6.0.7",
    "joi": "^18.0.2",
    "jsonwebtoken": "^9.0.3",
    "lru-cache": "^11.2.5",
    "mongoose": "^9.1.5",
    "pino": "^10.3.0",
    "pino-http": "^11.0.0",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@apollo/server": "^5.3.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.2.0",
    "@types/supertest": "^6.0.3",
    "@types/ws": "^8.18.1",
    "depcheck": "^1.4.7",
    "eslint": "^9.39.2",
    "eslint-plugin-import": "^2.32.0",
    "jest": "^30.2.0",
    "jscpd": "^4.0.8",
    "mongodb-memory-server": "^11.0.1",
    "npm-check-updates": "^19.3.2",
    "prettier": "^3.8.1",
    "rimraf": "^6.1.2",
    "supertest": "^7.2.2",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "ts-prune": "^0.10.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0"
  }
}
